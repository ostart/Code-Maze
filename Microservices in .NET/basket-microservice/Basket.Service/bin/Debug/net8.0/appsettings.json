{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMq": {
    "HostName": "localhost"
  },
  "EventBus": {
    "QueueName": "basket-microservice"
  },
  "Redis": {
    "Configuration": "host.docker.internal:6379"
  },
  "OpenTelemetry": {
    "OtlpExporterEndpoint": "http://host.docker.internal:4317"
  }
}
